{
  "name": "dcm-admin-panel",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@hookform/resolvers": "^2.9.7",
    "@mui/icons-material": "^5.4.2",
    "@mui/material": "^5.4.3",
    "@mui/styles": "^5.9.0",
    "@mui/x-data-grid": "^5.6.1",
    "@mui/x-date-pickers": "^5.0.0-beta.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "axios": "^0.26.1",
    "date-fns": "^2.28.0",
    "formik": "^2.2.9",
    "hamburger-react": "^2.5.0",
    "lodash.uniqwith": "^4.5.0",
    "moment": "^2.29.4",
    "notistack": "^2.0.3",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.33.1",
    "react-infinite-scroller": "^1.2.6",
    "react-player": "^2.11.0",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0",
    "react-window": "^1.8.7",
    "recoil": "^0.6.1",
    "recoilize": "^2.0.1",
    "sass": "^1.49.8",
    "uuid": "^3.3.2",
    "web-vitals": "^2.1.0",
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "indent": [
        "warn",
        2,
        {
          "SwitchCase": 1
        }
      ],
      "max-len": [
        "error",
        {
          "code": 120
        }
      ],
      "no-console": [
        "warn",
        {
          "allow": [
            "warn",
            "error"
          ]
        }
      ],
      "quotes": [
        "error",
        "single"
      ],
      "object-curly-spacing": [
        "warn",
        "always"
      ],
      "react/prop-types": [
        "error",
        {
          "ignore": [
            "children"
          ]
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
